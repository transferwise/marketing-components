{
  "name": "@transferwise/marketing-components-docs",
  "version": "4.7.0",
  "description": "TransferWise Design System",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "node ./docgen.js && next dev",
    "docs": "yarn dev",
    "build": "npm-run-all build:*",
    "build:docs-gen": "node ./docgen.js && node ./utils/bundleSize/getBundleSize.js",
    "build:next": "next build",
    "build:compile-css": "gulp compileLess --src='static/assets' --dest='static/assets'",
    "docs:deploy": "npm-run-all deploy:*",
    "deploy:export": "next export -o docs-build && touch docs-build/.nojekyll",
    "deploy:gh-pages": "deploy-to-github-pages -d docs-build",
    "lint": "eslint components/ pages/ utils/",
    "lint:fix": "yarn lint --fix",
    "test": "jest --config ../../node_modules/@transferwise/test-config/test/jest.lint.js"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/marketing-components",
    "url": "git+https://github.com/transferwise/marketing-components.git"
  },
  "keywords": [],
  "author": {
    "name": "TransferWise",
    "url": "https://transferwise.com/"
  },
  "bugs": {
    "url": "https://github.com/transferwise/marketing-components/issues"
  },
  "homepage": "https://transferwise.github.io/marketing-components",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@mapbox/rehype-prism": "^0.4.0",
    "@mdx-js/loader": "^1.5.8",
    "@next/mdx": "^9.0.5",
    "@transferwise/marketing-components": "^27.14.0",
    "@transferwise/dynamic-flows": "^2.2.28",
    "@transferwise/icons": "^1.5.0",
    "@transferwise/neptune-css": "^1.3.1",
    "@transferwise/test-config": "^1.0.3",
    "@zeit/next-css": "^1.0.1",
    "bootstrap": "github:transferwise/bootstrap#semver:^5.20.0",
    "currency-flags": "^2.1.1",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "lint-staged": "^9.4.2",
    "next": "^9.3.2",
    "next-fonts": "^1.0.3",
    "next-images": "^1.1.2",
    "next-transpile-modules": "^2.3.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "react-docgen": "5.3.0",
    "react-docgen-external-proptypes-handler": "^2.0.0",
    "react-live": "^2.2.2",
    "rehype-slug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@transferwise/less-config": "^1.0.4",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "deploy-to-github-pages": "1.0.0-beta.5"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "resolutions": {
    "lint-staged/**/kind-of": "^6.0.3",
    "next/**/kind-of": "^6.0.3"
  }
}
